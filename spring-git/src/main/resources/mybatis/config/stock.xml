<?xml version="1.0" encoding="UTF-8"?>
<!-- Sql Mapper -->
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="stock">
 	
 	<select id="getStockList" resultType="stockVO">
 		select symbol, price 
 		from stock
 	</select>
 	
 	
</mapper>
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
















